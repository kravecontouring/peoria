          "_customHTML": "<section group=\"Footers\" mbr-class=\"{'mbr-reveal': reveal, 'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->  \n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"0\">\n        <input type=\"checkbox\" title=\"Show Copyright\" name=\"showCopyright\" checked>\n        <select title=\"Icons\" name=\"iconsCount\">\n            <option value=\"0\">0</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <input type=\"checkbox\" title=\"Reveal effect\" name=\"reveal\">\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background4-1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#333333\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#3C3C3C\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container\">\n        <div class=\"media-container-row content text-white\">\n            <div class=\"col-12 col-md-3\">\n                <div class=\"media-wrap\">\n                    <a href=\"https://go.booker.com/location/kravetherapeutic/contact\" target=\"_blank\">\n                        <img src=\"@PROJECT_PATH@/assets/images/body-contouring-tree-onlylogo-192x233.jpeg\" alt title=\"Click me\">\n                    </a>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3 mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                <h5 class=\"pb-3\">\n                    <a href=\"https://go.booker.com/location/kravetherapeutic/contact\" class=\"text-primary\" target=\"_blank\">Address</a>\n                </h5>\n                <p class=\"mbr-text\">Krave Body Contouring @&nbsp;<br>Krave Therapeutic Massage<br>13760 N 93rd Ave, Suite 105<br>Peoria, Arizona &nbsp;85381</p>\n            </div>\n            <div class=\"col-12 col-md-3 mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                <h5 class=\"pb-3\">\n                    <a href=\"https://go.booker.com/location/kravetherapeutic/contact\" class=\"text-primary\" target=\"_blank\">Contact</a></h5>\n                <p class=\"mbr-text\"><a href=\"sms:+1-602-4025402\">(602) 402-5402</a> &nbsp; <br>Text Preferred &nbsp; &nbsp; &nbsp;<br>VM, please allow 24 hrs<br>kravecontouring@gmail.com<br></p>\n            </div>\n            <div class=\"col-12 col-md-3 mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                <h5 class=\"pb-3\"><a href=\"https://go.booker.com/location/kravetherapeutic/service-menu\" class=\"text-primary\" target=\"_blank\">By Appointment Only</a></h5>\n                <p class=\"mbr-text\">We do not take walk-ins. Appointment must be prescheduled. &nbsp;We recommend <a href=\"https://go.booker.com/location/kravetherapeutic/service-menu\" target=\"_blank\"><b>online booking</b></a>. &nbsp;Thank you.</p>\n            </div>\n        </div>\n        <div class=\"footer-lower\